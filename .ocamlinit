#use "ast.ml";;

#use "machine.ml";;

#use "compiler.ml";;

#use "eval.ml";;

let plus = Plus (Int 4, Plus (Int 9, Int 5));;

let vars = Let ("x", Int 4, Let ("y", Int 5, (Plus (Var "x", 
Plus (Int 9, Var "y")))));;

let more_vars = Let ("x", Int 1, Let ("y", Plus (Var "x", Int 3), 
Plus (Int 9, Var "y")))

let var_unused = Let ("x", Int 4, Let ("y", Int 5, (Plus (Var "x", Int 2))))

let identity = App (Lam ("x", Var "x"), Int 1)

let simple_app = App (Lam ("x", Plus (Int 2, Var "x")), Int 1)

let curry1 = App (Lam ("x", Lam ("y", Plus (Var "x", Var "y"))), Int 3)

let curry2 = Let ("func", 
App (Lam ("x", Lam ("y", Plus (Var "x", Var "y"))), Int 3),
App (Var "func", Int 8))